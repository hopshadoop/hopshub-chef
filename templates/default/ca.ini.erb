[hops-site]
url = <%= node['hopssite']['url'] %>
path-login = /hopsworks-api/api/auth/login
path-sign-cert = /hopsworks-ca/ca/agentservice/hopsworks
username = <%= node['hopssite']['user'] %>
password = <%= node['hopssite']['password'] %>
retry-interval = <%= node['hopssite']['retry_interval'] %>
max-retries = <%= node['hopssite']['max_retries'] %>
logging-level = "INFO"