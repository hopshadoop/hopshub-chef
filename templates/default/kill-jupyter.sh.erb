#!/bin/bash

help() {
    echo ""
    echo "usage: $0 kill port"
    echo ""
    exit 1
}


if [ $# -ne 1 ]; then
  help
fi

PORT=$1

function kill_port {
    PID=`ps aux | grep -i jupyter | grep $PORT | awk 'NR==1{print $2}'`
    if [ "$PID" != "" ] ; then
	kill -9 $PID > /dev/null 2>&1
        res=$?
    else
	res=$NOT_FOUND
    fi
    return $res
}

kill_port

exit $?